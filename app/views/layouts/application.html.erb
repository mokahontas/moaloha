<!DOCTYPE html>
<html>
<head>
  <title>Moaloha</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <link href='https://fonts.googleapis.com/css?family=Shadows+Into+Light|Exo|Bangers|Amatic+SC|Dancing+Script|Pacifico|Kaushan+Script|Courgette' rel='stylesheet' type='text/css'>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/skeleton/2.0.4/skeleton.min.css">
  <link href='https://fonts.googleapis.com/css?family=Economica:400,700' rel='stylesheet' type='text/css'>
  <script src="//maps.google.com/maps/api/js?v=3.23&key=AIzaSyDXXLsj4vNRXIeeoGRHtWR9IutXLhhxI5Y"></script>
  <script src="//cdn.rawgit.com/mahnunchik/markerclustererplus/master/dist/markerclusterer.min.js"></script>
  <script src='//cdn.rawgit.com/printercu/google-maps-utility-library-v3-read-only/master/infobox/src/infobox_packed.js' type='text/javascript'></script>

</head>
<body>
  <script>
  window.fbAsyncInit = function() {
    FB.init({
      appId      : '1776990429249548',
      xfbml      : true,
      version    : 'v2.6'
    });
  };

  (function(d, s, id){
     var js, fjs = d.getElementsByTagName(s)[0];
     if (d.getElementById(id)) {return;}
     js = d.createElement(s); js.id = id;
     js.src = "//connect.facebook.net/en_US/sdk.js";
     fjs.parentNode.insertBefore(js, fjs);
   }(document, 'script', 'facebook-jssdk'));
</script>
<%= image_tag("moalohalogo.png", class: "logo") %>

  <div class="navbar">
    <!-- <%= image_tag("moalohalogo.png", class: "headerimg") %> -->
    <button type="button" name="button"><%= link_to 'Home', pages_path, class: "btn" %></button>
    <button type="button" name="button"><%= link_to 'Resources', resources_path, class: "btn" %></button>
    <button type="button" name="button"><%= link_to 'Missing Person Database', missing_people_path, class: "btn" %></button>
    <button type="button" name="button"><%= link_to 'Social', social_path, class: "btn" %></button>
    <button type="button" name="button"><a href="#">Contact</a></button>
  </div>


<div class="container">

  <%= yield %>

</div>
<footer>
  <div
    class="fb-like"
    data-share="true"
    data-width="150"
    data-show-faces="true">
  </div>
  <button type="button" name="button"><%= link_to 'Admin', admins_path %></button>

  <% if  @current_user.present? %>
<button type="button" name="button">
  <%= link_to 'Log Out', logout_path, :method => :delete  %></button>

  <% end %>
</footer>

</body>
</html>
